{
    "env": {
        "browser": false,
        "commonjs": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", "tab", { "SwitchCase": 1 }],
        "no-const-assign": "error",
        "no-undef": "error",
        "no-unreachable": "warn",
        "no-unused-vars": "error",
        "valid-typeof": "warn"
    }
}
